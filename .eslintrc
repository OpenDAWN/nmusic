{
  "env": {
    "node": true
  },
  "rules": {
    "strict": [0],
    "semi": [2, "never"],
    "quotes": [2, "single"],
    "valid-jsdoc": [2, {
        "prefer": {
            "return": "returns"
        }
    }],
    "no-undef": [2],
    "no-use-before-define": [2],
    "curly": [0],
    "no-alert": [0],
    "new-cap": [0],
    "eol-last": [0],
    "no-shadow": [0],
    "no-empty": [0],
    "no-new": [0],
    "no-mixed-requires": [0],
    "no-loop-func": [0],
    "camelcase": [0],
    "consistent-return": [0],
    "no-underscore-dangle": [0],
    "key-spacing": [0]
  }
}